{
    "manifest_version": 2,
    "version": "1.0",
    "name": "MediaTab",
    "description": "Shows tabs playing media and integrates with Spotify API",

    "permissions": ["activeTab", "tabs"],

    "browser_action": {
        "default_icon": "icons/favicon.ico",
        "default_title": "MediaTab",
        "default_popup": "popup/index.html"
    }
}
